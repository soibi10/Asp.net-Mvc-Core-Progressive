@model IEnumerable<GaiboSoibifaSail.Models.AnnualFeeStructure>


@{
    ViewData["Title"] = "Annual fees by year";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Annual fees by year</h2>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.AnnualFee)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Year)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model.OrderByDescending(item => item.Year))
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.AnnualFee)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Year)
                </td>
                <td>
                    <a asp-action="Edit" asp-route-id="@item.Year">Edit</a> |
                    <a asp-action="Details" asp-route-id="@item.Year">Details</a> |
                    <a asp-action="Delete" asp-route-id="@item.Year">Delete</a>
                </td>
            </tr>
        }

    </tbody>
</table>